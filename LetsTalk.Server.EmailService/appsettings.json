{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "Url": "127.0.0.1:9092",
    "SendLoginCodeRequest": {
      "Topic": "send-login-code-request",
      "GroupId": "1"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "EmailService": {
    "SenderEmail": "",
    "SenderName": "",
    "Server": "",
    "Port": 0,
    "UserName": "",
    "Password": ""
  }
}
