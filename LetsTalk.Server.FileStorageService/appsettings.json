{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "https://127.0.0.1:5003",
        "Protocols": "Http2"
      }
    }
  },
  "FileStorage": {
    "BasePath": "%LetsTalk_Server_FileStorage%",
    "ImageFolder": "Images",
    "AvatarMaxWidth": 512,
    "AvatarMaxHeight": 512,
    "PictureMaxWidth": 4000,
    "PictureMaxHeight": 3000,
    "FilenameByImageIdCacheLifetime": "00:10:00"
  },
  "ConnectionStrings": {
    "DbConnectionString": "server=localhost; database=letstalk; user=root; password=hello@123"
  },
  "AuthenticationSettings": {
    "Url": "https://127.0.0.1:5001"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
