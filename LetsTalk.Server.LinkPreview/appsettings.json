{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "Url": "127.0.0.1:9092",
    "GroupId": "1"
  },
  "Aws": {
    "AccessKey": "",
    "SecretKey": "",
    "Region": ""
  },
  "Topics": {
    "LinkPreviewRequest": "letstalk-link-preview-request-topic"
  },
  "Queues": {
    "LinkPreviewRequest": "letstalk-link-preview-request-queue"
  },
  "ApplicationUrls": {
    "Api": "https://127.0.0.1:5000"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Features": {
    "LinkPreviewMode": "local",
    "EventBrokerMode": "kafka"
  }
}
