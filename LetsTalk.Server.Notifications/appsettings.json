{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ApplicationUrls": {
    "Authentication": "https://127.0.0.1:5001"
  },
  "Kafka": {
    "Url": "127.0.0.1:9092",
    "GroupId": "1"
  },
  "Aws": {
    "AccessKey": "",
    "SecretKey": "",
    "Region": ""
  },
  "Topics": {
    "MessageNotification": "letstalk-message-notification-topic",
    "LinkPreviewNotification": "letstalk-link-preview-notification-topic",
    "ImagePreviewNotification": "letstalk-image-preview-notification-topic"
  },
  "Queues": {
    "MessageNotification": "letstalk-message-notification-queue",
    "LinkPreviewNotification": "letstalk-link-preview-notification-queue",
    "ImagePreviewNotification": "letstalk-image-preview-notification-queue"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://127.0.0.1:5002",
        "Protocols": "Http1AndHttp2"
      }
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Features": {
    "EventBrokerMode": "Kakfa"
  }
}
