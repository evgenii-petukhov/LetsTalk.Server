{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Grpc": {
        "Url": "https://127.0.0.1:5003",
        "Protocols": "Http2"
      }
    }
  },
  "FileStorage": {
    "BasePath": "%LetsTalk_Server_FileStorage%",
    "ImageFolder": "Images",
    "AvatarMaxWidth": 512,
    "AvatarMaxHeight": 512
  }
}
